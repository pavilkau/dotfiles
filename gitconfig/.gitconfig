[alias]
  p = pull
	st = status
	co = checkout
  coma = checkout master
	br = branch
	cm = commit -m	
	pf = push --force-with-lease
	pushup = push -u origin HEAD
	amend = commit --amend
  pophead = reset --soft HEAD~1
[color]
	branch = auto
	diff = auto
	interactive = auto
	status = auto
[branch]
  autosetuprebase = always
[branch "master"]
  rebase = true
	pushRemote = no_push
[core]
	excludesfile = ~/.gitignore
